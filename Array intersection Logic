


#include<bits/stdc++.h>
using namespace std;


int main(){
    
    //Array intersection logic // Non optimised
    
    vector<int> ans;
    
    // iterate over first Array
    for(int i = 0; i<n;i++){
         int element = arr1[i];
         
         for(int i = 0; i<m; j++){
             if(element == arr2[j]){
                 ans.push_back(element);
                 
                 arr2[j] = -2;
                 break;
             }
         }
    }
    return ans;
}
