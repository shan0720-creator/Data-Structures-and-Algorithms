#include <iostream>
#include<math.h>

using namespace std;




int main()
{    
    
    int num  = 2;
    
    char ch = 'a';
    
    switch(ch){
        case 1: cout<<"Case1"<<endl;
        
        break;
        
        case 2: cout<<"Case2"<<endl;
        
        break;
        
        case 'a': cout<<"a"<<endl;
        
        default: "Default case";
    }
 
   
}
